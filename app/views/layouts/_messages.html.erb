<div class="row">
	<div class="col-xs-10 col-xs-offset-1">
		<% flash.each do |notice, msg| %>
		<div class='alert alert-<%= "#{notice}" %>' id="msg_box">
			<a href="#" class="close" data-dismiss="alert">&#215;</a>
			<ul><li style="list-style: none"><%= msg %></li></ul>
		</div>
		<% end %>
	</div>
</div>


